  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/pg": "^8.6.6",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.0",
